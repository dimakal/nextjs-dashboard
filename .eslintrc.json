{
     "extends": [
          "next",
          "prettier/prettier",
          "prettier",
          "plugin:prettier/recommended"
     ],
     "plugins": ["prettier"],
     "rules": {
          "prettier/prettier": [
               "error"
          ],
          "max-len": ["error", { "code": 120, "tabWidth": 2, "ignoreStrings": false }]
     }
}